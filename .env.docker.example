# PostgreSQL Configuration
POSTGRES_DB=exlo
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your-secure-password
POSTGRES_PORT=5432

# Web Dashboard Configuration
WEB_PORT=3000
BETTER_AUTH_SECRET=your-auth-secret-min-32-chars
BETTER_AUTH_URL=http://localhost:3000

# Internal API Configuration (shared between web and tunnl services)
# Must be at least 32 characters in production
INTERNAL_API_SECRET=your-internal-api-secret-min-32-chars

# Tunnl Configuration
SSH_PORT=2222
PROXY_URL=http://localhost:8080
RUST_LOG=info

# Optional Tunnl Configuration (uncomment to customize)
# MGMT_PORT=9090
# CODE_EXPIRY_SECS=300
# POLL_INTERVAL_SECS=2
# MAX_POLL_ATTEMPTS=150
# SERVER_KEY_PATH=server_key.pem
