# Domain Configuration (using .localhost for automatic resolution)
DOMAIN=exlo.localhost
TUNNEL_DOMAIN=tunnel.localhost

# Web Dashboard Configuration
BETTER_AUTH_SECRET=local-dev-secret-min-32-chars!!

# Tunnl Configuration
SSH_PORT=2222
RUST_LOG=debug
